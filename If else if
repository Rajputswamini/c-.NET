*if else if*                               using System.IO;
using System;
public class demo 
{
    int no=12;
    public void get()
    {
    if(no<5)
    {
Console. WriteLine("12 is less than 5");
    }
    else if(no>5)
    {
Console. WriteLine("12 is greater than 5");
    }
    else
    {
Console.WriteLine("12 is equal to 5");        
    }
    }
}
class Program
{
 public    static void Main(string[]args)
    {
        demo d=new demo();
        d. get();
        Console.ReadLine();
    }
}
