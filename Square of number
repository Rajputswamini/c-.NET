using System.IO;
using System;
public class demo
{
    int a, b; 
    public void read()
    {
Console. WriteLine("enter the numbers ");
a=Convert.ToInt32(Console.ReadLine());
    }
public void display()
{
b=a*a;
Console.WriteLine("square"+b);
}

class Program
{
  public   static void Main(string[]args)
    {
demo d=new demo();
d. read();
d. display();
Console. ReadLine();
    }
}
}

 *To print square number*
