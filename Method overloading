using System.IO;
using System;
public class  Calculator 
{
    public int add(int a,int b)
    {
        return a+b;
    }
public double add(double a,double b)
{ 
    return a+b;
}
public int add(int a,int b,int c)
{
    return a+b+c;
}
}
public class demo
{
    static void Main()
    {
        Calculator calculator=new Calculator();
     int result1=calculator.add(1,2);
      double  result2=calculator.add(1.5,2.5);
      int result3=calculator.add(1,2,3);
      
    Console.WriteLine("result of adding 1 & 2"+result1);
Console.WriteLine("result of adding 1.5 & 2.5"+result2);
Console.WriteLine("result of adding 1,2 & 3"+result3);
    
    }
}     

*Method overloading*
