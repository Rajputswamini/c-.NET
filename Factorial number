using System.IO;
using System;
public class demo 
{
int number, i, fact=1;
public void read()
{
Console.WriteLine("enter the numbers ");
number=Convert.ToInt32(Console.ReadLine());
}
public void display()
{
for(i=1;i<=number; i++)
{
fact=fact*i;
}
     Console. WriteLine("factorial of:-"+fact);
}
}

class Program
{
   public  static void Main(string[]args)
    {
        demo d=new demo();
        d.read();
        d.display();
        Console.ReadLine();
    }
}   

 *Factorial number*
