. *If* 
using System.IO;
using System;
public class demo 
{
    int no=2;
    public void get()
    {
    if(no<5)
    {
Console. WriteLine("2 is less than 5");
    }
    }
}
class Program
{
 public    static void Main(string[]args)
    {
        demo d=new demo();
        d. get();
        Console.ReadLine();
    }
}
